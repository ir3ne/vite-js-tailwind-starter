{
  "name": "vite-js-starter",
  "private": false,
  "version": "1.0.1",
  "description": "A Vite.js starter template with JavaScript and Tailwind CSS",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "format": "prettier . --write"
  },
  "keywords": [
    "vite",
    "javascript",
    "tailwind",
    "npm"
  ],
  "files": [
    "index.html",
    "index.js",
    "style.css",
    "tailwind.config.js",
    "postcss.config.js"
  ],
  "author": "ir3ne",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "only-allow": "^1.2.1",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
